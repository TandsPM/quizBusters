<body>
  <!-- Top nav bar (fixed) -->
  <nav>
    <!-- <img id="icon" src="../images/logo2_transparent.png"> -->
      <img id="icon" src="https://media.discordapp.net/attachments/1174046026684956762/1185402453638782986/quizBUSTERS.png?ex=658f7b10&is=657d0610&hm=c49d96e43a11c7a56199a8b8e9f5b14f2c598ea720cad8cfcad5e41a7cbabaa1&=&format=webp&quality=lossless&width=1146&height=1146">
    <span id="quizBusters"> quizBusters</span>

    <div class="search-container">
      <textarea name="search" id="navSearch" class="search-input" placeholder="Search..."></textarea>
      <button type="submit" class="search-button">Search</button>
    </div>

    <div class="navButtons">
      <button type="link" value="myQuizzes">My Quizzes</button>
      <button type="link" value="myResults">My Results</button>
      <button type="link" value="myFaves">My Faves</button>
      <button type="link" value="me">ME!</button>
      <i class="fa-solid fa-angles-down fa-beat-fade" style="color: whitesmoke;"></i>
    </div>
  </nav>
  <!-- Page-specific (main) content here -->
  <section id="page-content">
    <header id="page-header">  <!-- user-specific content here -->
      <div>
        <!-- <img id="logo" src="../images/logo2.png"> -->
        <img id="logo" src="https://media.discordapp.net/attachments/1174046026684956762/1185403482371530782/quizBUSTERS2.png?ex=658f7c05&is=657d0705&hm=ec9ca01f8f3e4e3ce9314179cb544f38720d6c47bff660814aacd61cd62fc84e&=&format=webp&quality=lossless&width=1146&height=1146">
      </div>
      <br />
      <div class="app-info">
        <h2>quizBusters!</h2>
      </div>
    </header>
    <main class="container">
      <section class="thatQuiz">
        <label for="thatQuiz" class="thatQuiz-title"> What Kinda Cookie are You?</label>
        <br/>
        <label for="thatQuiz" class="thatQuiz-author"> by Jane Doe</label>
        <br/>
        <div class="quiz-content-demo">
          <div class="question">do you prefer rain or sun?</div>
          <fieldset>
            <div class="answer-options">
              <div class="answer-option"><input type="radio" name="rain" value="rain" /> i like rain!</div>
              <div class="answer-option"><input type="radio" name="sun" value="sun" /> i like sun!</div>
              <div class="answer-option"><input type="radio" name="both" value="both" /> i like both!</div>
          </fieldset>
          </div>

          <div class="question">do you prefer books or movies?</div>
          <fieldset>
            <div class="answer-options">
              <div class="answer-option"><input type="radio" name="books" value="books" /> i like books!</div>
              <div class="answer-option"><input type="radio" name="movies" value="movies" /> i like movies!</div>
              <div class="answer-option"><input type="radio" name="both" value="both" /> i like both!</div>
          </fieldset>
        </div>
        <!-- <time class="thatQuiz-time">${ago}</time> -->
        <div class="reactions">
          <div class="reaction"><i class="fa-regular fa-flag"></i></div>
          <div class="reaction"><i class="fa-solid fa-retweet"></i></div>
          <div class="reaction"><i class="fa-regular fa-heart"></i></div>
        </div>
      </section>
      <section class="thatQuiz">
        <label for="thatQuiz" class="thatQuiz-title"> Demo Quiz Title</label>
        <br/>
        <label for="thatQuiz" class="thatQuiz-author"> by John Deere</label>
        <br/>
        <div class="quiz-content-demo">
          <div class="question">do you prefer rain or sun?</div>
          <fieldset>
            <div class="answer-options">
              <div class="answer-option"><input type="radio" name="rain" value="rain" /> i like rain!</div>
              <div class="answer-option"><input type="radio" name="sun" value="sun" /> i like sun!</div>
              <div class="answer-option"><input type="radio" name="both" value="both" /> i like both!</div>
          </fieldset>
          </div>

          <div class="question">do you prefer books or movies?</div>
          <fieldset>
            <div class="answer-options">
              <div class="answer-option"><input type="radio" name="books" value="books" /> i like books!</div>
              <div class="answer-option"><input type="radio" name="movies" value="movies" /> i like movies!</div>
              <div class="answer-option"><input type="radio" name="both" value="both" /> i like both!</div>
          </fieldset>
        </div>
        <!-- <time class="thatQuiz-time">${ago}</time> -->
        <div class="reactions">
          <div class="reaction"><i class="fa-regular fa-flag"></i></div>
          <div class="reaction"><i class="fa-solid fa-retweet"></i></div>
          <div class="reaction"><i class="fa-regular fa-heart"></i></div>
        </div>
      </section>
    </main>
  </section>
</body>

</html>
